                      &#956;&#941;&#955;&#953;&#964;&#964;&#945;&#953;, &#8068;&#948;&#959;&#965;&#963;&#953; &#948;&#8125; &#7952;&#957; &#964;&#945;&#8150;&#962; &#955;&#972;&#967;&#956;&#945;&#953;&#962; &#959;&#7985; &#8004;&#961;&#957;&#953;&#952;&#949;&#962;. &#954;&#945;&#964;&#8048; &#964;&#945;&#8016;&#964;&#8048; &#959;&#8022;&#957; &#959;&#7989; &#964;&#949;
                      &#960;&#945;&#8150;&#948;&#949;&#962; &#954;&#945;&#8054; &#945;&#7985; &#954;&#972;&#961;&#945;&#953; &#956;&#953;&#956;&#959;&#8166;&#957;&#964;&#945;&#953; &#964;&#8048; &#947;&#953;&#947;&#957;&#972;&#956;&#949;&#957;&#945;: &#7936;&#954;&#959;&#973;&#959;&#957;&#964;&#949;&#962; &#956;&#8050;&#957; &#8064;&#948;&#972;&#957;&#964;&#969;&#957; &#964;&#8182;&#957;
                      &#8000;&#961;&#957;&#943;&#952;&#969;&#957; &#8068;&#948;&#959;&#965;&#963;&#953; &#954;&#945;&#8054; &#945;&#8016;&#964;&#959;&#943;: &#946;&#955;&#941;&#960;&#959;&#957;&#964;&#949;&#962; &#948;&#8050; &#963;&#954;&#953;&#961;&#964;&#8182;&#957;&#964;&#945;&#962; &#964;&#959;&#8058;&#962; &#7940;&#961;&#957;&#945;&#962;, &#7941;&#955;&#955;&#959;&#957;&#964;&#945;&#953;
                      &#954;&#959;&#973;&#966;&#959;&#953;&#962; &#964;&#959;&#8150;&#962; &#960;&#959;&#963;&#943;&#957;: &#964;&#8048;&#962; &#948;&#8050; &#956;&#949;&#955;&#943;&#964;&#964;&#945;&#962; &#956;&#953;&#956;&#959;&#973;&#956;&#949;&#957;&#959;&#953; &#964;&#8048; &#7940;&#957;&#952;&#951; &#963;&#965;&#955;&#955;&#941;&#947;&#959;&#965;&#963;&#953;&#957;,
